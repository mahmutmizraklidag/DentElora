@model IEnumerable<Treatment>

@{


    var treatment = Model;




    <li class="dropdown">
        <a asp-controller="Treatments" asp-action="Index">Tedaviler</a>
        <ul>
            @foreach (var downmenu in treatment)
            {
                <li>
                    <a asp-controller="Treatments" asp-action="Detail" asp-route-slug="@downmenu.Slug">@downmenu.Title</a>

                </li>
            }
           
        </ul>
    </li>
}